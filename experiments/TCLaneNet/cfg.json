{
  "device": "cuda:0",
  "MAX_EPOCHES": 200,
  "model": "TCLaneNet",

  "dataset": {
    "dataset_name": "mydata",
    "batch_size": 8,
    "resize_shape": [800, 288]
  },

  "optim": {
    "lr": 5e-3,
    "momentum": 0.9,
    "weight_decay": 2e-5,
    "nesterov": true
  },

  "lr_scheduler": {
    "warmup": 50,
    "max_iter": 25000
  },

  "sad_start_iter": 20000,

  "evaluate": {
    "test": "yes",
    "model_path": "./experiments/TCLaneNet/TCLaneNet_iou_best.pth"
  }
}
